function createRTBIframe(params){var iframe=document.createElement('iframe');iframe.width=1;iframe.height=1;iframe.scrolling="no";iframe.frameBorder=0;iframe.setAttribute("style","display: none;");var iframeSrc="//creativecdn.com/tags?id=pr_S10QdRFxD7D0Bd4Np10M";switch(params.page){case"biblio_book":iframeSrc+="_offer_"+params.artId;break;case"purchase":var cd=true;var UserPartnerID=window.UserPartnerID||'';if(UserPartnerID=="260030908"){cd=false;}
iframeSrc+="_orderstatus2_"+params.value+"_"+params.order_id+"_"+params.artId+"&cd="+cd;break;case"basket":iframeSrc+="_basketstatus_"+params.artId;break;case"category":iframeSrc+="_category2_"+params.id;break;}
iframe.src=iframeSrc;document.body.appendChild(iframe);}
function sendToActionPay(){if(parseInt(UserPartnerID,10)===4787605){for(var i in ya_orders){var goods=ya_orders[i].order.goods
if(ya_orders[i].target=='order'){var fullprice=0;for(var j in goods){fullprice+=goods[j].price;}
var pixel=new Image();var actionpay=window.location.search.match(new RegExp('actionpay'+'=([^&=#]+)'))||'';if(actionpay&&actionpay[1]){actionpay=actionpay[1];}
pixel.src='//apypx.com/ok/litres.png?actionpay='+actionpay+'&apid='+ya_orders[i].order.order_id+'&price='+fullprice;}}}
sent_actionpay=1;}
function setBoughtCookie(){for(var i in ya_orders){if(ya_orders[i].target=='order'){setCookie('just_bought','1',0,'/',0,15);break;}}
set_bought_cookie=1;}